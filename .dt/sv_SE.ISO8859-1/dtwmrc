###
#
# Root Menu Description
#
###

Menu DtRootMenu
{
    "Menyn Arbetsyta"			f.title
    
# Raderna nedan genereras av dtaction GenerateRootMenu. Ändra dem inte.
# Linjer under har genererats av dtaction GenerateRootMenu. Redigera inte dessa.
# vvv dtaction GenerateRootMenu vvv
    "Filer"		f.menu FilerMenyn
    "Hjälp"		f.menu HjälpMenyn
    "Kort"		f.menu KortMenyn
    "Länkar"		f.menu LänkarMenyn
    "Mappar"		f.menu MapparMenyn
    "Post"		f.menu PostMenyn
    "Program"		f.menu ProgramMenyn
    "Verktyg"		f.menu VerktygMenyn
    "Värdar"		f.menu VärdarMenyn
# ^^^ dtaction GenerateRootMenu ^^^
# Linjer över har genererats av dtaction GenerateRootMenu. Redigera inte dessa.
# Raderna ovan genereras av dtaction GenerateRootMenu. Ändra dem inte.
    	no-label				f.separator
	"Fönster"				f.menu WindowsMenu
	no-label				f.separator
	"Lägg till alternativ till meny..."	f.action AddItemToMenu
	"Anpassa meny..."			f.action CustomizeWorkspaceMenu
	no-label				f.separator
	"Lås bildskärm"				f.action LockDisplay

    	"Viloläge..."				f.action SDTsuspend

    	"Logga ut..."				f.action ExitSession
}

Menu WindowsMenu
{
    	"Fönster"				f.title
	"Flytta uppåt"				f.circle_up
	"Flytta nedåt"				f.circle_down
	"Uppdatera"				f.refresh
	"Rensa ikoner"				f.pack_icons
	no-label				f.separator
	"Minimera/återställ huvudgruppen"	f.toggle_frontpanel
	"Återställ Arbetsytehanteraren..."	f.restart
	"Uppdatera menyn Arbetsyta"		f.action UpdateWorkspaceMenu
	no-label				f.separator
	"Deaktivera behörighetskontroll"	f.action Xhost+
	"Aktivera behörighetskontroll"		f.action Xhost-
	no-label				f.separator
	"Avbryt fönster..."			f.exec /usr/openwin/bin/xkill
}

